{
  "name": "translate-me",
  "displayName": "Translate Me",
  "description": "Detecta strings fixas em projetos Flutter.",
  "version": "0.0.1",
  "publisher": "seuPublisherID",
  "engines": {
    "vscode": "^1.50.0"
  },
  "activationEvents": [
    "onLanguage:dart",
    "onCommand:translate-me.scan",
    "onCommand:translate-me.toggle",
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "translate-me.scan",
        "title": "Translate Me: Scan"
      },
      {
        "command": "translate-me.toggle",
        "title": "Translate Me: Toggle Detection"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "^14.14.6",
    "@types/vscode": "^1.50.0",
    "eslint": "^7.10.0",
    "globby": "^11.0.4",
    "typescript": "^4.0.3",
    "vscode-test": "^1.4.0"
  },
  "dependencies": {
    "globby": "^11.0.4"
  }
}